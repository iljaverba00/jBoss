<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jboss-web PUBLIC
		"-//JBoss//DTD Web Application 4.2//EN"
		"http://www.jboss.org/j2ee/dtd/jboss-web_4_2.dtd">

<jboss-web>

	<security-domain>java:/jaas/geocad</security-domain>
	<context-root>/arcgis</context-root>

	<!-- установка типа аутентификации и класса-обработчика -->
	<authenticators>
		<authenticator>
			<key>GEEARCGIS</key>
			<value>com.geocad.wc.sfw.arcgis.ArcGisAuthenticator</value>
		</authenticator>
	</authenticators>

</jboss-web>